{
    "name": "All-In-One Chrome Extension",
    "description": "Ad-Blocker, Picture-in-Picture, Currency Exchange, Weather Updates, Daily-News and etc.. ",
    "version": "1.0.5",
    "icons": {
      "128": "assets/multiArrowIcon.png"
    },
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_icon": {
        "19": "assets/multiArrowIcon.png"
      },
      "default_popup": "popup.html"  
    },
    "commands": {
      "_execute_action": {
        "suggested_key": {
          "windows": "Alt+P",
          "mac": "Alt+P",
          "chromeos": "Alt+P",
          "linux": "Alt+P"
        }
      }
    },
    "permissions": [
      "scripting",
      "storage"
    ],
    "host_permissions": [
      "<all_urls>"
    ],
    "manifest_version": 3
  }
  