{
  "name": "TEST-All-In-One Chrome Extension",
  "description": "Ad-Blocker, Picture-in-Picture, Currency Exchange, Weather Updates, Daily-News, etc.",
  "version": "1.0.7",
  "icons": {
    "128": "assets/multiArrowIcon.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_icon": {
      "19": "assets/multiArrowIcon.png"
    },
    "default_popup": "index.html"
  },
  "permissions": [
    "scripting",
    "storage",
    "declarativeNetRequest",
    "geolocation"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "manifest_version": 3
}
